{
    "name": "ohm",
    "displayName": "Ohm Language",
    "description": "Ohm language support for Visual Studio Code",
    "author": "Attila Hajdrik",
    "version": "1.0.0",
    "publisher": "attilah",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/attilah/ohm-vscode"
    },
    "contributes": {
        "languages": [{
            "id": "ohm",
            "aliases": ["Ohm", "ohm"],
            "extensions": [".ohm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ohm",
            "scopeName": "source.ohm",
            "path": "./syntaxes/ohm.tmLanguage.json"
        }]
    }
}